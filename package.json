{
  "name": "stars-ts",
  "version": "1.0.0",
  "description": "Map of the stars",
  "main": "",
  "scripts": {
    "clean": "rm -rf dist node_modules src/data/precomputation.json",
    "pretty": "prettier --write 'src/**/*.ts*'",
    "start:dev": "webpack-dev-server --mode development",
    "test": "tsc --noEmit && jest --no-cache",
    "precomputation": "tsc && node  dist/pre-computation/pre-computation.js src/data/hygdata_v3.csv src/data/constellation.json > src/data/precomputation.json",
    "prestart:dev": "npm run precomputation",
    "deploy": "npm run clean && npm i && npm run precomputation && webpack --mode production &&  gh-pages -d dist"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx)"
    ]
  },
  "author": "@blemoine",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/d3": "^5.0.0",
    "@types/geojson": "^7946.0.4",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.109",
    "@types/node": "^10.10.3",
    "@types/papaparse": "^4.5.3",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "fast-check": "^1.5.0",
    "gh-pages": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "prettier": "^1.14.2",
    "style-loader": "^0.23.0",
    "ts-jest": "^23.1.4",
    "ts-loader": "^4.5.0",
    "typescript": "^3.0.3",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  },
  "dependencies": {
    "@types/rc-tooltip": "^3.4.12",
    "d3": "^5.7.0",
    "lodash": "^4.17.10",
    "papaparse": "^4.6.0",
    "rc-tooltip": "^3.7.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  }
}
